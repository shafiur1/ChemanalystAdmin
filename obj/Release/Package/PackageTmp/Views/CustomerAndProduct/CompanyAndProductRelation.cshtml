@model IEnumerable<ChemAnalyst.Models.CompanyAndProductRelation>
@{
    ViewBag.Title = "CompanyAndProductRelation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="update-profile-wrapper">


        <div class="white-wrapper" style="padding-top:10px">

            <div class="data-detail-wrapper table-responsive">

                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>@Html.DisplayNameFor(c => c.SA_Company.Name)</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.SA_Company.Name</td>
                            </tr>
                        }
                    </tbody>
                </table>

            </div>

        </div>

    </div>
