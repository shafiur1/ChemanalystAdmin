

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no, shrink-to-fit=no" />
    <title>Chem Analyst</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/bootstrap-admin.css" type="text/css" />
    <link rel="stylesheet" href="~/css/style2.css" type="text/css" />
    <link rel="stylesheet" href="~/css/build.css" type="text/css" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet" />
    @*<script src="~/js/jquery.js"></script>*@
    <script src="~/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/css/custom.css" type="text/css" />

</head>
<body oncontextmenu="return false;">
    <script>
        $(document).ready(function () {

            $(this).bind("contextmenu", function (e) {

                e.preventDefault();

            });


        });

    </script>
    <section class="top-wrapper">
        <div style="text-align: center; background-color:rgba(0, 0, 0, .075);">

            <img src="~/images/logo.png" alt="" />
        </div>
        <div class="top-inner">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="top-left">
                            <span>
                                <i class="fa fa-bars"></i>
                            </span>

                        </div>
                    </div>

                    <div class="col-sm-6">
                        <div class="top-right">

                            <h4>@Session["User"].ToString()</h4>
                            <img src="~/@Session["UserImg"].ToString()" alt="" width="50" height="50" />

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    @*@{
        List<ChemAnalyst.Models.CustWiseAccess> objAccess = (List<ChemAnalyst.Models.CustWiseAccess>)@Session["Access"];*@
    <div class="side-wrapper">

        <ul>

            <li>
                <a href="~/ChemicalPricing/ChecmPriceChartuser">
                    <i class="fa fa-rupee"></i>Chemical Pricing
                </a>
            </li>

            <li>
                <a href="~/MarketAnalysis/MarketAnalysisChartUser">
                    <i class="fa fa-bar-chart"></i>Market Analysis
                </a>
            </li>

            <li>
                <a href="@Url.Action("CustCompaniesList","Login")">
                    <i class="fa fa-building-o"></i> Company Profile
                </a>

            </li>

            <li>
                <a href="@Url.Action("CustIndustryList","Login")">
                    <i class="fa fa-file-text"></i>Industry Report
                </a>
            </li>

            <li>
                <a href="~/NewsAndDeals/NewsHome">
                    <i class="fa fa-newspaper-o"></i>News
                </a>
            </li>

            <li>
                <a href="~/NewsAndDeals/DealsHome">
                    <i class="fa fa-ticket"></i>Deals
                </a>
            </li>


            <li>
                <a href="@Url.Action("ShowCustSubscriptionListForSales", "SubsManagement")">
                    <i class="fa fa-shopping-cart"></i>My Subscriptions
                </a>
            </li>
            <li>
                <a href="@Url.Action("CustUpdateProfile", "Login")">
                    <i class="fa fa-user"></i>Profile
                </a>
            </li>
            <li>
                <a href="@Url.Action("ChangePassword", "Login")">
                    <i class="fa fa-lock"></i>Change Password
                </a>
            </li>

            <li>
                <a data-toggle="collapse" data-target="#pcmenu" class="drop-menu" aria-expanded="true" href="#">
                    <i class="fa fa-link"></i>Useful Links
                    <span class="pull-right-container">
                        <i class="fa fa-angle-left pull-right"></i>
                    </span>
                </a>
                <ul class="sub-menu collapse" id="pcmenu" aria-expanded="true" style="">
                    <li>
                        <a target="_blank" href="@Url.Action("Faq", "ChemAnalyst")">
                            <i class="fa fa-question-circle"></i>FAQs
                        </a>
                    </li>
                    <li>
                        <a target="_blank" href="@Url.Action("Privacypolicy", "ChemAnalyst")">
                            <i class="fa fa-key"></i>Privacy Policy
                        </a>
                    </li>
                    <li>
                        <a target="_blank" href="@Url.Action("Terms", "ChemAnalyst")">
                            <i class="fa fa-download"></i>Terms &amp; Conditions
                        </a>
                    </li>
                    <li>
                        <a target="_blank" href="@Url.Action("AboutUs", "ChemAnalyst")">
                            <i class="fa fa-info"></i>About ChemAnalyst
                        </a>
                    </li>


                </ul>
            </li>
            <li>
                <a href="@Url.Action("LogOut", "User")">
                    <i class="fa fa-sign-out"></i>Logout
                </a>
            </li>

        </ul>
    </div>


    <div class="right-contant-bar">
        @RenderBody()
    </div>
    <div class="clearfix fillbluedark"></div>
    <footer>
        <div class="container">
            <div class="copyright">
                @*DATE TIME SHOULD BE*@
                <p>Copyright © @DateTime.Now.Year Chem Analyst Pvt. Ltd. All rights reserved.</p>
            </div>
        </div>
    </footer>
    @RenderSection("scripts", required: false)
    @RenderSection("script", required: false)
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">






    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    <script>
        $(function () {

            $("#datepicker").datepicker({ dateFormat: "yy/mm/dd" }).datepicker("setDate", new Date());
        });
    </script>
    <script src="~/js/customer_responsive.js"></script>
    <script src="~/js/active.js"></script>

    <!--Scripts for map-->
    <script src="~/js/raphael.min.js"></script>
    <script src="~/js/mapael.js"></script>
    <script src="~/js/india_states.js"></script>
    <script src="~/js/india_states_legends.js"></script>
</body>
</html>
