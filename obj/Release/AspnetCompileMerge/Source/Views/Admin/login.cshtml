<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no, shrink-to-fit=no">
    <title>Chem Analyst - Admin Login </title>
    <link rel="stylesheet" href="~/css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="~/css/style.css" type="text/css" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet">
</head>
<body>
    <section class="main-bg-02">
        <div class="container">
            <div class="clearfix" style="height: 30px"></div>
            <div class="system-main-wrapper">
                <div class="row nlogin">
                    <div class="loginBox">
                        <div id="login">
                            <form action="~/Admin/Userlogin" method="post">
                                <fieldset class="clearfix">
                                    <p><input type="text" Placeholder="Username" name="Username" required><span class="fa fa-envelope"></span></p>
                                    <p><input type="password" Placeholder="Password" name="Password" required><span class="fa fa-lock"></span></p>
                                    <span><input type="submit" value="Sign In"></span>
                                </fieldset>
                                <div class="clearfix"></div>
                            </form>
                            <div class="clearfix"></div>
                            @{
                                if (TempData["ErrorMessage"] != null)
                                {
                                    <span style="color: #962929;">@TempData["ErrorMessage"].ToString() </span>
                                }
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <footer>
        <div class="container">
            <div class="copyright">
                <p>Copyright © 2019 Chem Analyst Pvt. Ltd. All rights reserved.</p>
            </div>
        </div>
    </footer>
</body>
<script src="js/jquery.js"></script>
<script src="js/bootstrap.min.js"></script>

</html>
